{
  "name": "savedx",
  "productName": "savedX",
  "version": "1.0.0",
  "description": "The X bookmark manager",
  "main": "src/main.mjs",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite --config vite.config.js",
    "start": "concurrently -k -n VITE,ELECTRON -c blue,green \"vite --config vite.config.js\" \"wait-on http://localhost:5173 && electron .\"",
    "package": "rm -rf chrome_data && rm -rf dist && npm run build:renderer && cp -r dist .vite/ && electron-packager . savedX --platform=darwin --arch=arm64 --overwrite --out=out",
    "build:renderer": "vite build --config vite.config.js",
    "lint": "eslint --ignore-pattern 'noupload/*' --ignore-pattern '.vite/*'",
    "format": "prettier --write \"src/**/*.{js,jsx}\""
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.7",
    "@mui/material": "^6.0.2",
    "@table-library/react-table-library": "^4.1.7",
    "autoprefixer": "^10.4.19",
    "cheerio": "^1.0.0-rc.12",
    "crypto": "^1.0.1",
    "dompurify": "^3.1.0",
    "dotenv": "^16.4.5",
    "electron": "29.2.0",
    "electron-log": "^5.2.4",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.21.1",
    "get-port": "^7.1.0",
    "postcss": "^8.4.38",
    "prettier": "^3.4.2",
    "puppeteer": "^22.6.2",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.6",
    "react-loader-spinner": "^6.1.6",
    "sqlite3": "^5.1.7",
    "xbot-js": "^1.1.8"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.0.3",
    "concurrently": "^9.1.2",
    "electron-packager": "^17.1.1",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.2",
    "globals": "^15.9.0",
    "tailwindcss": "^3.4.3",
    "vite": "^5.0.12",
    "wait-on": "^8.0.3"
  }
}
